    "aws-sdk": "^2.1500.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next-auth": "^4.24.5",
    "release-please": "^16.2.0",
    "web": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "@types/cookie-session": "^2.0.47",
    "@types/cors": "^2.8.16",
